    "_isA11yCompletionDescriptionEnabled": true,
    "title": "New Component Title",
    "displayTitle": "New Component Title",
    "body": "<p>Why does the AI sound confident even when the answer is incorrect?</p>",
    "_ariaLevel": 0,
    "_id": "69429459c9430b433c292551",
    "_parentId": "69429427c9430b433c292550",
    "_type": "component",
    "_layout": "full",
    "_onScreen": {
      "_isEnabled": false,
